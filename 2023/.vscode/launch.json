    "version": "0.2.0",
    "configurations": [
        {
            "name": "Node.js - Debug Current File",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "preLaunchTask": "build"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/app.js",
            "preLaunchTask": "build"
        }
    ]
}